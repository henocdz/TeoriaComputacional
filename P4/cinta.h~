#ifndef __CINTA_H__
#define __CINTA_H__

#include <stdio.h>
#include <stdlib.h>

typedef struct cinta 
	{ 
	struct nodo *posicion;
	}Cinta;
typedef struct nodo 
	{
	struct nodo *anterior;
	struct nodo *siguiente;
	int simbolo;
	}Nodo;

Nodo* crear(void);
int mover_derecha (struct cinta *mi_cinta);
int mover_izquierda (struct cinta *mi_cinta);
int leer_simbolo (struct cinta *mi_cinta);
int escribir_simbolo (struct cinta *mi_cinta,int simbolo);
#endif
