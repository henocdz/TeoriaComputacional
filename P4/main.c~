#include "cinta.h"
#include <stdio.h>
#include <stdlib.h>

int verificar(cinta);

int main (void)
{
	int a;
	int b;
	a=leer(File *);

	b=verificar();
}

int verificar(Cinta c){ 

	estado *temporal; 

	int contadora,contadorb,contador; 

	temporal=c; 

	contadora=contadorb=contador=0; 

	while(temporal!=i){ 

	if(temporal->simbolo=='a')
		contadora++;
	if(temporal->simbolo=='b')
		contadorb++;
	if(temporal->simbolo=='c')
		contador++;

	temporal=temporal->siguiente; 

} 

if((contadorb+contadora)==contador) 
	return 1; 
else 
	return 0; 

} 

